<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="C64Black">#000000</Color>
    <Color x:Key="C64White">#FFFFFF</Color>
    <Color x:Key="C64Red">#813338</Color>
    <Color x:Key="C64Cyan">#75CEC8</Color>
    <Color x:Key="C64Purple">#8E3C97</Color>
    <Color x:Key="C64Green">#56AC4D</Color>
    <Color x:Key="C64Blue">#2E2C9B</Color>
    <Color x:Key="C64Yellow">#EDF171</Color>
    <Color x:Key="C64Orange">#8E5029</Color>
    <Color x:Key="C64Brown">#553800</Color>
    <Color x:Key="C64LightRed">#C46C71</Color>
    <Color x:Key="C64DarkGray">#4A4A4A</Color>
    <Color x:Key="C64Gray">#7B7B7B</Color>
    <Color x:Key="C64LightGreen">#A9FF9F</Color>
    <Color x:Key="C64LightBlue">#706DEB</Color>
    <Color x:Key="C64LightGray">#B2B2B2</Color>

    <SolidColorBrush x:Key="C64BlackBrush" Color="{StaticResource C64Black}"/>
    <SolidColorBrush x:Key="C64WhiteBrush" Color="{StaticResource C64White}"/>
    <SolidColorBrush x:Key="C64RedBrush" Color="{StaticResource C64Red}"/>
    <SolidColorBrush x:Key="C64CyanBrush" Color="{StaticResource C64Cyan}"/>
    <SolidColorBrush x:Key="C64PurpleBrush" Color="{StaticResource C64Purple}"/>
    <SolidColorBrush x:Key="C64GreenBrush" Color="{StaticResource C64Green}"/>
    <SolidColorBrush x:Key="C64BlueBrush" Color="{StaticResource C64Blue}"/>
    <SolidColorBrush x:Key="C64YellowBrush" Color="{StaticResource C64Yellow}"/>
    <SolidColorBrush x:Key="C64OrangeBrush" Color="{StaticResource C64Orange}"/>
    <SolidColorBrush x:Key="C64BrownBrush" Color="{StaticResource C64Brown}"/>
    <SolidColorBrush x:Key="C64LightRedBrush" Color="{StaticResource C64LightRed}"/>
    <SolidColorBrush x:Key="C64DarkGrayBrush" Color="{StaticResource C64DarkGray}"/>
    <SolidColorBrush x:Key="C64GrayBrush" Color="{StaticResource C64Gray}"/>
    <SolidColorBrush x:Key="C64LightGreenBrush" Color="{StaticResource C64LightGreen}"/>
    <SolidColorBrush x:Key="C64LightBlueBrush" Color="{StaticResource C64LightBlue}"/>
    <SolidColorBrush x:Key="C64LightGrayBrush" Color="{StaticResource C64LightGray}"/>

</ResourceDictionary>
